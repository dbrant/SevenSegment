<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>net8.0-windows7.0</TargetFramework>
    <OutputType>Library</OutputType>
    <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
    <UseWindowsForms>true</UseWindowsForms>
    <ImportWindowsDesktopTargets>true</ImportWindowsDesktopTargets>
    <Authors>Dmitry Brant</Authors>
    <Description>A full-featured seven-segment control for Windows Forms.</Description>
    <Copyright>Copyright © 2009- Dmitry Brant</Copyright>
    <PackageProjectUrl>https://dmitrybrant.com</PackageProjectUrl>
  </PropertyGroup>
  <ItemGroup>
    <Compile Update="SevenSegment.cs">
      <SubType>UserControl</SubType>
    </Compile>
    <Compile Update="SevenSegmentArray.cs">
      <SubType>UserControl</SubType>
    </Compile>
  </ItemGroup>
</Project>